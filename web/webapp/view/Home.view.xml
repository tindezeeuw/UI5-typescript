<mvc:View controllerName="ui5typescriptdemo.controller.Home" xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
                    <List
                        items="{/list}">
                        <StandardListItem title="{title}" />
                    </List>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>